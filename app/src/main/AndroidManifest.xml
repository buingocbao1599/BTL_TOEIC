        android:supportsRtl="true"
        android:theme="@style/Theme.BTL_Toeic">
        <activity android:name=".ActivityChonNgonNgu"></activity>
        <activity android:name=".ActivityPhanHoi" />
        <activity android:name=".ActivityThongTinApp" />
        <activity android:name=".ActivityMuctieu" />
        <activity android:name=".ActivityChucmung" />
        <activity android:name=".ActivityTratu" />
        <activity android:name=".ActivityOntap" />
        <activity android:name=".ActivityHoctumoi" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>